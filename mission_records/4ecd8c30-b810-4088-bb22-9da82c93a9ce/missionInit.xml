<?xml version="1.0" encoding="UTF-8" standalone="no" ?>
<MissionInit xmlns="http://ProjectMalmo.microsoft.com" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://ProjectMalmo.microsoft.com MissionInit.xsd">
  <Mission>
    <About>
      <Summary>Hello world!</Summary>
    </About>
    <ServerSection>
      <ServerInitialConditions>
        <Time>
          <StartTime>18000</StartTime>
          <AllowPassageOfTime>false</AllowPassageOfTime>
        </Time>
        <Weather>clear</Weather>
      </ServerInitialConditions>
      <ServerHandlers>
        <FlatWorldGenerator forceReset="false" generatorString="3;1*minecraft:bedrock,7*minecraft:dirt,1*minecraft:grass;2;" seed=""/>
        <DrawingDecorator>
          <DrawCuboid type="obsidian" x1="1" x2="7" y1="8" y2="12" z1="1" z2="7"/>
          <DrawCuboid type="air" x1="2" x2="6" y1="9" y2="11" z1="2" z2="6"/>
          <DrawCuboid type="air" x1="1" x2="7" y1="9" y2="10" z1="4" z2="4"/>
          <DrawCuboid type="air" x1="7" x2="7" y1="9" y2="10" z1="4" z2="4"/>
          <DrawCuboid type="air" x1="4" x2="4" y1="9" y2="10" z1="1" z2="1"/>
          <DrawCuboid type="air" x1="4" x2="4" y1="9" y2="10" z1="7" z2="7"/>
          <DrawBlock type="redstone_block" x="4" y="9" z="4"/>
          <DrawBlock type="air" x="4" y="8" z="4"/>
          <DrawBlock type="diamond_block" x="4" y="7" z="4"/>
        </DrawingDecorator>
        <ServerQuitWhenAnyAgentFinishes description=""/>
      </ServerHandlers>
    </ServerSection>
    <AgentSection mode="Survival">
      <Name>Burton</Name>
      <AgentStart>
        <Placement pitch="0" x="0.5" y="9" yaw="0" z="0.5"/>
      </AgentStart>
      <AgentHandlers>
        <ObservationFromFullStats/>
        <ObservationFromGrid>
          <Grid name="floorGrid">
            <min x="-10" y="-1" z="-10"/>
            <max x="10" y="1" z="10"/>
          </Grid>
        </ObservationFromGrid>
        <ObservationFromChat/>
        <ObservationFromNearbyEntities>
          <Range name="Nearby" update_frequency="1" xrange="2" yrange="2" zrange="2"/>
          <Range name="Mob" update_frequency="100" xrange="10" yrange="2" zrange="10"/>
          <Range name="Player" update_frequency="100" xrange="100" yrange="2" zrange="100"/>
        </ObservationFromNearbyEntities>
        <VideoProducer viewpoint="0" want_depth="false">
          <Width>1920</Width>
          <Height>1200</Height>
        </VideoProducer>
        <ContinuousMovementCommands turnSpeedDegs="180">
          <ModifierList type="deny-list">
            <command>strafe</command>
          </ModifierList>
        </ContinuousMovementCommands>
        <ChatCommands/>
        <AgentQuitFromTouchingBlockType>
          <Block description="" type="diamond_block"/>
        </AgentQuitFromTouchingBlockType>
      </AgentHandlers>
    </AgentSection>
    <AgentSection mode="Survival">
      <Name>Gertrude</Name>
      <AgentStart>
        <Placement pitch="0" x="-4.5" y="9" yaw="0" z="0.5"/>
      </AgentStart>
      <AgentHandlers>
        <ObservationFromChat/>
        <ContinuousMovementCommands turnSpeedDegs="840">
          <ModifierList type="deny-list">
            <command>strafe</command>
          </ModifierList>
        </ContinuousMovementCommands>
        <ChatCommands/>
        <AgentQuitFromTouchingBlockType>
          <Block description="" type="diamond_block"/>
        </AgentQuitFromTouchingBlockType>
      </AgentHandlers>
    </AgentSection>
  </Mission>
  <ExperimentUID/>
  <ClientRole>0</ClientRole>
  <ClientAgentConnection>
    <ClientIPAddress>127.0.0.1</ClientIPAddress>
    <ClientMissionControlPort>10000</ClientMissionControlPort>
    <ClientCommandsPort>0</ClientCommandsPort>
    <AgentIPAddress>127.0.0.1</AgentIPAddress>
    <AgentMissionControlPort>10195</AgentMissionControlPort>
    <AgentVideoPort>10921</AgentVideoPort>
    <AgentObservationsPort>10152</AgentObservationsPort>
    <AgentRewardsPort>10473</AgentRewardsPort>
  </ClientAgentConnection>
</MissionInit>
